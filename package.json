{
  "name": "adoc_generate",
  "description": "Scaffolding tool to generate AsciiDoctor files in the Antora structure.",
  "version": "0.0.4-hotfix",
  "author": "Lucian Ciolacu",
  "bin": {
    "adoc_generate": "./bin/run"
  },
  "bugs": "https://github.com/lucianciolacu/adoc_generate/issues",
  "dependencies": {
    "@oclif/command": "^1.5.17",
    "@oclif/config": "^1.13.2",
    "@oclif/plugin-help": "^2.2.0"
  },
  "devDependencies": {
    "@oclif/dev-cli": "^1.22.0",
    "eslint": "^5.16.0",
    "eslint-config-oclif": "^3.1.0",
    "globby": "^10.0.1"
  },
  "engines": {
    "node": ">=8.0.0"
  },
  "files": [
    "/bin",
    "/npm-shrinkwrap.json",
    "/oclif.manifest.json",
    "/templates",
    "/src"
  ],
  "homepage": "https://github.com/lucianciolacu/adoc_generate",
  "keywords": [
    "oclif"
  ],
  "license": "MIT",
  "main": "src/index.js",
  "oclif": {
    "commands": "./src/commands",
    "bin": "adoc_generate",
    "plugins": [
      "@oclif/plugin-help"
    ]
  },
  "repository": "lucianciolacu/adoc_generate",
  "scripts": {
    "postpack": "rm -f oclif.manifest.json",
    "posttest": "eslint .",
    "prepack": "oclif-dev manifest && oclif-dev readme",
    "test": "echo NO TESTS",
    "version": "oclif-dev readme && git add README.md"
  }
}
